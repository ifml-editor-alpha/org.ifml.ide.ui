 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          allPopups="false"
          locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
       <toolbar
             id="org.ifml.ide.ui.IfmlToolbar"
             label="IFML">
          <command
                commandId="org.ifml.ide.ui.NewViewPointCommand"
                icon="icons/full/obj16/viewpoint_obj.png"
                label="New View Point"
                style="push">
             <visibleWhen
                   checkEnabled="false">
                <reference
                      definitionId="org.ifml.ide.ui.IfmlEditorIsActive">
                </reference>
             </visibleWhen>
          </command>
       </toolbar>
    </menuContribution>
 </extension>
 -->
 <extension
       point="org.eclipse.core.expressions.definitions">
    <definition
          id="org.ifml.ide.ui.IfmlEditorIsActive">
       <with
             variable="activeEditorId">
          <equals
                value="org.ifml.ide.ui.editors.IfmlModelEditor">
          </equals>
       </with>
    </definition>
 </extension>
</plugin>
